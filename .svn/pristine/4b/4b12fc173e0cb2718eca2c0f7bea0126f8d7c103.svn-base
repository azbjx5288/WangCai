package com.wangcai.lottery.data;

public class SpinnerEditBean {
    public int id;
    public String name;

    public SpinnerEditBean() {
    }

    public SpinnerEditBean(int id, String name) {
        this.id = id;
        this.name = name;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @Override
    public String toString() {
        return name;
    }

}
