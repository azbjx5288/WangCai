<?xml version="1.0" encoding="utf-8"?>
<com.wangcai.lottery.component.DisInterceptNestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/title_bar_height"
    android:clipChildren="false"
    android:clipToPadding="false"
    app:layout_collapseMode="parallax"
    app:layout_collapseParallaxMultiplier="0.8">

    <LinearLayout
        android:id="@+id/user_head_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <fragment
            android:id="@+id/uc_zoomiv"
            android:name="com.wangcai.lottery.component.CycleViewPager"
            android:layout_width="match_parent"
            android:layout_height="@dimen/banner_height"
            android:scaleType="centerCrop"
            android:tag="overScroll" />

    </LinearLayout>

</com.wangcai.lottery.component.DisInterceptNestedScrollView>