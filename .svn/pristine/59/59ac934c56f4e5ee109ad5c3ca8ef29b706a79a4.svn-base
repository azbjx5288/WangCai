package com.wangcai.lottery.data;

public class RedoubleRowData {
    private int id;
    private String issue;
    private String issueAbridge;
    private Integer multiple;
    private double cost;
    private String times;

    public RedoubleRowData(int id, String issue, String issueAbridge, Integer multiple, double cost, String times) {
        this.id = id;
        this.issue = issue;
        this.issueAbridge = issueAbridge;
        this.multiple = multiple;
        this.cost = cost;
        this.times = times;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getIssue() {
        return issue;
    }

    public void setIssue(String issue) {
        this.issue = issue;
    }

    public String getIssueAbridge() {
        return issueAbridge;
    }

    public void setIssueAbridge(String issueAbridge) {
        this.issueAbridge = issueAbridge;
    }

    public Integer getMultiple() {
        return multiple;
    }

    public void setMultiple(Integer multiple) {
        this.multiple = multiple;
    }

    public double getCost() {
        return cost;
    }

    public void setCost(double cost) {
        this.cost = cost;
    }

    public String getTimes() {
        return times;
    }

    public void setTimes(String times) {
        this.times = times;
    }
}
