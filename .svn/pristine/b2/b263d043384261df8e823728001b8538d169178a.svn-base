package com.wangcai.lottery.data;

import com.google.gson.annotations.SerializedName;

/**
 * Created by ACE-PC on 2016/6/24.
 */
public class RechargeChannelConfig {

    /**
     * id : 14
     * name : 乐付网银
     * payment_type_id : 1
     * type_name : 网银
     * need_bank : 1
     * is_qr : 0
     * save_qr : 0
     * min_amount : 1.00
     * max_amount : 3000.00
     */

    @SerializedName("id")
    private int id;
    @SerializedName("name")
    private String name;
    @SerializedName("payment_type_id")
    private int paymentTypeId;
    @SerializedName("type_name")
    private String typeName;
    @SerializedName("need_bank")
    private int needBank;
    @SerializedName("is_qr")
    private boolean isQr;
    @SerializedName("save_qr")
    private int saveQr;
    @SerializedName("min_amount")
    private double minAmount;
    @SerializedName("max_amount")
    private double maxAmount;

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getPaymentTypeId() {
        return paymentTypeId;
    }

    public void setPaymentTypeId(int paymentTypeId) {
        this.paymentTypeId = paymentTypeId;
    }

    public String getTypeName() {
        return typeName;
    }

    public void setTypeName(String typeName) {
        this.typeName = typeName;
    }

    public int getNeedBank() {
        return needBank;
    }

    public void setNeedBank(int needBank) {
        this.needBank = needBank;
    }

    public boolean isQr() {
        return isQr;
    }

    public void setQr(boolean qr) {
        isQr = qr;
    }

    public int getSaveQr() {
        return saveQr;
    }

    public void setSaveQr(int saveQr) {
        this.saveQr = saveQr;
    }

    public double getMinAmount() {
        return minAmount;
    }

    public void setMinAmount(double minAmount) {
        this.minAmount = minAmount;
    }

    public double getMaxAmount() {
        return maxAmount;
    }

    public void setMaxAmount(double maxAmount) {
        this.maxAmount = maxAmount;
    }
}
