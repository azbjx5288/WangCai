<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:bottom="11.5dp">
        <rotate
            android:fromDegrees="300"
            android:pivotX="85%"
            android:pivotY="45%"
            android:toDegrees="0">
            <shape>
                <solid android:color="@color/app_main" />
                <size
                    android:width="50dp"
                    android:height="2dp" />
                <stroke
                    android:width="1dp"
                    android:color="@color/app_main" />
            </shape>
        </rotate>
    </item>
    <item android:top="11.5dp">
        <rotate
            android:fromDegrees="60"
            android:pivotX="85%"
            android:pivotY="45%"
            android:toDegrees="0">
            <shape>
                <solid android:color="@color/app_main" />
                <size
                    android:width="50dp"
                    android:height="2dp" />
                <stroke
                    android:width="1dp"
                    android:color="@color/app_main" />
            </shape>
        </rotate>
    </item>

    <item>
        <rotate
            android:fromDegrees="90"
            android:pivotX="90%"
            android:pivotY="100%"
            android:toDegrees="0">
            <shape>
                <solid android:color="@color/app_main" />
                <size
                    android:width="50dp"
                    android:height="70dp" />
                <stroke
                    android:width="1dp"
                    android:color="@color/app_main" />
            </shape>
        </rotate>
    </item>
</layer-list>