<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="0"
            android:pivotX="50%"
            android:pivotY="100%"
            android:toDegrees="0">
            <shape>
                <solid android:color="@color/app_main" />
                <stroke
                    android:width="1dp"
                    android:color="@color/app_main" />
            </shape>
        </rotate>
    </item>
    <item android:bottom="11.5dp" android:left="-5dp">
        <rotate
            android:fromDegrees="315"
            android:pivotX="0%"
            android:pivotY="45%"
            android:toDegrees="0">
            <shape>
                <solid android:color="@color/black" />
                <stroke
                    android:width="1dp"
                    android:color="@color/black" />
            </shape>
        </rotate>
    </item>
    <item android:top="11.5dp" android:left="-5dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="0%"
            android:pivotY="45%"
            android:toDegrees="0">
            <shape>
                <solid android:color="@color/black" />
                <stroke
                    android:width="1dp"
                    android:color="@color/black" />
            </shape>
        </rotate>
    </item>
</layer-list>